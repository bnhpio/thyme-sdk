  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@rslib/core": "^0.17.1",
    "@rstest/core": "^0.6.1",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3"
  },
  "private": true,
  "dependencies": {
    "ajv": "^8.17.1",
    "viem": "^2.38.6",
    "zlib": "^1.0.5"
