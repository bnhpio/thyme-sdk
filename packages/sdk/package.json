{
  "name": "@bnhpio/thyme-sdk",
  "version": "1.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./task/archive": {
      "types": "./dist/task/archive/index.d.ts",
      "import": "./dist/task/archive/index.js",
      "require": "./dist/task/archive/index.cjs"
    },
    "./task/transaction": {
      "types": "./dist/task/transaction/index.d.ts",
      "import": "./dist/task/transaction/index.js",
      "require": "./dist/task/transaction/index.cjs"
    },
    "./task/schema": {
      "types": "./dist/task/schema/index.d.ts",
      "import": "./dist/task/schema/index.js",
      "require": "./dist/task/schema/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
